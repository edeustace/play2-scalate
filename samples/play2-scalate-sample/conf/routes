# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /mustache                   controllers.Application.indexWithMustache(name:String="Dali")
GET     /mustache/:name             controllers.Application.indexWithMustache(name:String)

GET     /jade                       controllers.Application.indexWithJade()

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
